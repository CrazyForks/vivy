{
  "name": "vivy",
  "version": "0.1.0",
  "description": "Stable diffusion desktop application",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "vite build --watch --config vite.main.ts --mode=development",
    "dev:renderer": "vite",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "vite build --config vite.main.ts",
    "build:renderer": "vite build",
    "start": "electron ./dist/main/index.js",
    "start:easy-diffusion": "node ./easy-diffusion/start.mjs",
    "format": "prettier \"easy-diffusion/env/*.mjs\" \"src/**/*.{ts,tsx}\" \"*.{ts,json}\" --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liriliri/vivy.git"
  },
  "author": "surunzi",
  "bugs": {
    "url": "https://github.com/liriliri/vivy/issues"
  },
  "homepage": "https://github.com/liriliri/vivy#readme",
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.0.1",
    "electron": "^24.1.2",
    "electron-builder": "^23.6.0",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.3.1"
  },
  "dependencies": {
    "execa": "^7.1.1",
    "licia": "^1.38.1"
  }
}
