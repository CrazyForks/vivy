{
  "name": "VIVY",
  "version": "0.9.0",
  "description": "Stable diffusion desktop application",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:preload\" \"npm run dev:renderer\"",
    "dev:main": "vite build --watch --config vite.main.ts --mode=development",
    "dev:preload": "vite build --watch --config vite.preload.ts --mode=development",
    "dev:renderer": "vite",
    "build": "zx script/build.mjs",
    "pack": "zx script/pack.mjs",
    "upload": "zx script/upload.mjs",
    "suggestions": "zx script/suggestions.mjs",
    "build:main": "vite build --config vite.main.ts",
    "build:preload": "vite build --config vite.preload.ts",
    "build:renderer": "vite build",
    "start": "electron ./dist/main/index.js",
    "format": "lsla prettier \"webui/env/*.mjs\" \"src/**/*.{ts,tsx,scss,css,json}\" \"*.{ts,json}\" \"script/*.mjs\" --write",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "genIcon": "lsla genIcon --input src/renderer/icon --output src/renderer/icon.css --name vivy-icon",
    "genTheme": "lsla genTheme --input src/common/theme.json --output src/renderer/theme.scss && lsla genTheme --input src/common/theme.json --output src/common/theme.ts",
    "genPb": "cd src/renderer/main/lib && pbjs -t static-module -w es6 -o vivyFile.js vivyFile.proto && pbts -o vivyFile.d.ts vivyFile.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liriliri/vivy.git"
  },
  "author": "surunzi",
  "bugs": {
    "url": "https://github.com/liriliri/vivy/issues"
  },
  "homepage": "https://github.com/liriliri/vivy#readme",
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.529.1",
    "@monaco-editor/react": "^4.5.2",
    "@types/exif": "^0.6.3",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "axios": "^1.4.0",
    "concurrently": "^8.0.1",
    "custom-electron-titlebar": "^4.2.7",
    "electron": "^24.1.2",
    "electron-builder": "^23.6.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "execa": "^7.1.1",
    "exif": "^0.6.0",
    "exifr": "^7.1.3",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "install": "^0.13.0",
    "licia": "^1.39.1",
    "luna-cropper": "^0.2.1",
    "luna-data-grid": "^0.6.0",
    "luna-image-viewer": "^0.4.0",
    "luna-mask-editor": "^0.4.1",
    "luna-menu": "^0.1.2",
    "luna-modal": "^1.2.1",
    "luna-notification": "^0.1.6",
    "luna-painter": "^0.4.0",
    "luna-performance-monitor": "^0.4.1",
    "luna-setting": "^0.9.4",
    "luna-tab": "^0.3.0",
    "luna-toolbar": "^0.5.6",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^4.0.2",
    "monaco-editor": "^0.25.2",
    "npm": "^10.5.0",
    "pidusage": "^3.0.2",
    "png-chunks-extract": "^1.0.0",
    "protobufjs-cli": "^1.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "typescript": "^5.1.6",
    "vite": "^4.3.1",
    "xterm": "^5.2.1",
    "xterm-addon-canvas": "^0.4.0",
    "xterm-addon-fit": "^0.7.0",
    "xterm-addon-unicode11": "^0.5.0",
    "xterm-addon-webgl": "^0.15.0",
    "zx": "^7.2.2"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "protobufjs": "^7.2.6"
  }
}
