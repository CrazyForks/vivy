{
  "name": "vivy",
  "version": "0.1.0",
  "description": "Stable diffusion desktop application",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:preload\" \"npm run dev:renderer\"",
    "dev:main": "vite build --watch --config vite.main.ts --mode=development",
    "dev:preload": "vite build --watch --config vite.preload.ts --mode=development",
    "dev:renderer": "vite",
    "build": "npm run build:main && npm run preload && npm run build:renderer",
    "build:main": "vite build --config vite.main.ts",
    "build:preload": "vite build --config vite.preload.ts",
    "build:renderer": "vite build",
    "start": "electron ./dist/main/index.js",
    "format": "prettier \"easy-diffusion/env/*.mjs\" \"webui/env/*.mjs\" \"src/**/*.{ts,tsx,scss}\" \"*.{ts,json}\" --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liriliri/vivy.git"
  },
  "author": "surunzi",
  "bugs": {
    "url": "https://github.com/liriliri/vivy/issues"
  },
  "homepage": "https://github.com/liriliri/vivy#readme",
  "dependencies": {
    "stable-diffusion-api": "^0.0.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.4",
    "@vitejs/plugin-react": "^4.0.0",
    "axios": "^1.4.0",
    "concurrently": "^8.0.1",
    "electron": "^24.1.2",
    "electron-builder": "^23.6.0",
    "execa": "^7.1.1",
    "licia": "^1.38.1",
    "luna-setting": "^0.7.0",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^4.0.2",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "vite": "^4.3.1"
  }
}
